                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="N/A"
                    android:textColor="@color/text_color"
                    android:textSize="18dp" />

            </LinearLayout>

            <LinearLayout
